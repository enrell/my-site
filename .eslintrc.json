{
  "extends": "next/core-web-vitals",
  "rules": {
    "semi": ["error", "always"],
    "indent": ["error", 4],
    "no-tabs": ["error", false],
    "quotes": ["error", "double"],
    "no-unused-vars": "warn",
    "no-console": "off"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  }
}
